<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from BankServiceService WSDL service -->

  <service name="BankService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://services.web"/>
      <parameter name="wsdlServiceElement" value="BankServiceService"/>
      <parameter name="schemaQualified" value="http://models.web,http://services.web"/>
      <parameter name="wsdlServicePort" value="BankService"/>
      <parameter name="className" value="web.services.BankServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="BankService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="description" qname="operNS:description" xmlns:operNS="http://services.web" returnQName="retNS:descriptionReturn" xmlns:retNS="http://services.web" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="hasFunds" qname="operNS:hasFunds" xmlns:operNS="http://services.web" returnQName="retNS:hasFundsReturn" xmlns:retNS="http://services.web" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:bc" xmlns:pns="http://services.web" type="tns:BankClient" xmlns:tns="http://models.web"/>
        <parameter qname="pns:value" xmlns:pns="http://services.web" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="removeFunds" qname="operNS:removeFunds" xmlns:operNS="http://services.web" soapAction="" >
        <parameter qname="pns:bc" xmlns:pns="http://services.web" type="tns:BankClient" xmlns:tns="http://models.web"/>
        <parameter qname="pns:value" xmlns:pns="http://services.web" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="addClient" qname="operNS:addClient" xmlns:operNS="http://services.web" soapAction="" >
        <parameter qname="pns:bc" xmlns:pns="http://services.web" type="tns:BankClient" xmlns:tns="http://models.web"/>
        <parameter qname="pns:value" xmlns:pns="http://services.web" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getFunds" qname="operNS:getFunds" xmlns:operNS="http://services.web" returnQName="retNS:getFundsReturn" xmlns:retNS="http://services.web" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:bc" xmlns:pns="http://services.web" type="tns:BankClient" xmlns:tns="http://models.web"/>
      </operation>
      <parameter name="allowedMethods" value="hasFunds addClient description getFunds removeFunds"/>

      <typeMapping
        xmlns:ns="http://models.web"
        qname="ns:Product"
        type="java:web.models.Product"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://services.web"
        qname="ns:ArrayOf_tns1_Product"
        type="java:web.models.Product[]"
        innerType="cmp-ns:Product" xmlns:cmp-ns="http://models.web"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://models.web"
        qname="ns:Rating"
        type="java:web.models.Rating"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://models.web"
        qname="ns:BankClient"
        type="java:web.models.BankClient"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
